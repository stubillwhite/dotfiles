--langdef=ant
--langmap=ant:.xml
--regex-ant=/<\s*macrodef\s*name\s*=\s*"([a-zA-Z0-9 -.]+)"/\1/,macrodef/i
--regex-ant=/<\s*path\s*id\s*=\s*"([a-zA-Z0-9 -.]+)"/\1/,path/i
--regex-ant=/<\s*property\s*name\s*=\s*"([a-zA-Z0-9 -.]+)"/\1/,property/i
--regex-ant=/<\s*target\s*name\s*=\s*"([a-zA-Z0-9 -.]+)"/\1/,target/i
--langdef=properties
--langmap=properties:.properties
--regex-properties=/^([^=]+)=/\1/,property/
--python-kinds=-i
